{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "ApimServiceName": {
      "type": "string",
      "metadata": {
        "description": "Name of the API Management"
      }
    },
    "LinkedTemplatesBaseUrl": {
      "type": "string",
      "metadata": {
        "description": "Base URL of the repository"
      }
    }
  },
  "resources": [
    {
      "type": "Microsoft.Resources/deployments",
      "properties": {
        "mode": "Incremental",
        "templateLink": {
          "uri": "[concat(parameters('LinkedTemplatesBaseUrl'), '/apiVersionSets.template.json')]",
          "contentVersion": "1.0.0.0"
        },
        "parameters": {
          "ApimServiceName": {
            "value": "[parameters('ApimServiceName')]"
          }
        }
      },
      "name": "versionSetTemplate",
      "apiVersion": "2019-05-01",
      "dependsOn": []
    },
    {
      "type": "Microsoft.Resources/deployments",
      "properties": {
        "mode": "Incremental",
        "templateLink": {
          "uri": "[concat(parameters('LinkedTemplatesBaseUrl'), '/backends.template.json')]",
          "contentVersion": "1.0.0.0"
        },
        "parameters": {
          "ApimServiceName": {
            "value": "[parameters('ApimServiceName')]"
          }
        }
      },
      "name": "backendsTemplate",
      "apiVersion": "2019-05-01",
      "dependsOn": []
    },
    {
      "type": "Microsoft.Resources/deployments",
      "properties": {
        "mode": "Incremental",
        "templateLink": {
          "uri": "[concat(parameters('LinkedTemplatesBaseUrl'), '/products.template.json')]",
          "contentVersion": "1.0.0.0"
        },
        "parameters": {
          "ApimServiceName": {
            "value": "[parameters('ApimServiceName')]"
          }
        }
      },
      "name": "productsTemplate",
      "apiVersion": "2019-05-01",
      "dependsOn": []
    },
    {
      "type": "Microsoft.Resources/deployments",
      "properties": {
        "mode": "Incremental",
        "templateLink": {
          "uri": "[concat(parameters('LinkedTemplatesBaseUrl'), '/ehsan-ebillinghub-ValidationApiV1-initial.api.template.json')]",
          "contentVersion": "1.0.0.0"
        },
        "parameters": {
          "ApimServiceName": {
            "value": "[parameters('ApimServiceName')]"
          }
        }
      },
      "name": "ValidationApiV1-InitialAPITemplate",
      "apiVersion": "2019-05-01",
      "dependsOn": [
        "[resourceId('Microsoft.Resources/deployments', 'versionSetTemplate')]",
        "[resourceId('Microsoft.Resources/deployments', 'productsTemplate')]",
        "[resourceId('Microsoft.Resources/deployments', 'backendsTemplate')]"
      ]
    },
    {
      "type": "Microsoft.Resources/deployments",
      "properties": {
        "mode": "Incremental",
        "templateLink": {
          "uri": "[concat(parameters('LinkedTemplatesBaseUrl'), '/ehsan-ebillinghub-ValidationApiV1-subsequent.api.template.json')]",
          "contentVersion": "1.0.0.0"
        },
        "parameters": {
          "ApimServiceName": {
            "value": "[parameters('ApimServiceName')]"
          }
        }
      },
      "name": "ValidationApiV1-SubsequentAPITemplate",
      "apiVersion": "2019-05-01",
      "dependsOn": [
        "[resourceId('Microsoft.Resources/deployments', 'ValidationApiV1-InitialAPITemplate')]"
      ]
    },
    {
      "type": "Microsoft.Resources/deployments",
      "properties": {
        "mode": "Incremental",
        "templateLink": {
          "uri": "[concat(parameters('LinkedTemplatesBaseUrl'), '/ehsan-ebillinghub-SubmissionApiV1-initial.api.template.json')]",
          "contentVersion": "1.0.0.0"
        },
        "parameters": {
          "ApimServiceName": {
            "value": "[parameters('ApimServiceName')]"
          }
        }
      },
      "name": "SubmissionApiV1-InitialAPITemplate",
      "apiVersion": "2019-05-01",
      "dependsOn": [
        "[resourceId('Microsoft.Resources/deployments', 'versionSetTemplate')]",
        "[resourceId('Microsoft.Resources/deployments', 'productsTemplate')]",
        "[resourceId('Microsoft.Resources/deployments', 'backendsTemplate')]",
        "[resourceId('Microsoft.Resources/deployments', 'ValidationApiV1-SubsequentAPITemplate')]"
      ]
    },
    {
      "type": "Microsoft.Resources/deployments",
      "properties": {
        "mode": "Incremental",
        "templateLink": {
          "uri": "[concat(parameters('LinkedTemplatesBaseUrl'), '/ehsan-ebillinghub-SubmissionApiV1-subsequent.api.template.json')]",
          "contentVersion": "1.0.0.0"
        },
        "parameters": {
          "ApimServiceName": {
            "value": "[parameters('ApimServiceName')]"
          }
        }
      },
      "name": "SubmissionApiV1-SubsequentAPITemplate",
      "apiVersion": "2019-05-01",
      "dependsOn": [
        "[resourceId('Microsoft.Resources/deployments', 'SubmissionApiV1-InitialAPITemplate')]"
      ]
    },
    {
      "type": "Microsoft.Resources/deployments",
      "properties": {
        "mode": "Incremental",
        "templateLink": {
          "uri": "[concat(parameters('LinkedTemplatesBaseUrl'), '/ehsan-ebillinghub-StatusApiV1-initial.api.template.json')]",
          "contentVersion": "1.0.0.0"
        },
        "parameters": {
          "ApimServiceName": {
            "value": "[parameters('ApimServiceName')]"
          }
        }
      },
      "name": "StatusApiV1-InitialAPITemplate",
      "apiVersion": "2019-05-01",
      "dependsOn": [
        "[resourceId('Microsoft.Resources/deployments', 'versionSetTemplate')]",
        "[resourceId('Microsoft.Resources/deployments', 'productsTemplate')]",
        "[resourceId('Microsoft.Resources/deployments', 'backendsTemplate')]",
        "[resourceId('Microsoft.Resources/deployments', 'SubmissionApiV1-SubsequentAPITemplate')]"
      ]
    },
    {
      "type": "Microsoft.Resources/deployments",
      "properties": {
        "mode": "Incremental",
        "templateLink": {
          "uri": "[concat(parameters('LinkedTemplatesBaseUrl'), '/ehsan-ebillinghub-StatusApiV1-subsequent.api.template.json')]",
          "contentVersion": "1.0.0.0"
        },
        "parameters": {
          "ApimServiceName": {
            "value": "[parameters('ApimServiceName')]"
          }
        }
      },
      "name": "StatusApiV1-SubsequentAPITemplate",
      "apiVersion": "2019-05-01",
      "dependsOn": [
        "[resourceId('Microsoft.Resources/deployments', 'StatusApiV1-InitialAPITemplate')]"
      ]
    }
  ]
}